	-- GESTOR DE MODOS
	-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
	local GestorModos = {}
	local modoActual = "VISUAL"

	function GestorModos:CambiarModo(modo)
		-- Ocultar todos los elementos
		contenedorMiniMapa.Visible = false
		panelMatrizAdyacencia.Visible = false
		overlayAnalisis.Visible = false
		btnMisiones.Visible = false
		misionFrame.Visible = false
		btnAlgoritmo.Visible = false
		btnMapa.Visible = false

		-- Mostrar segÃºn el modo
		if modo == "VISUAL" then
			contenedorMiniMapa.Visible = true
			btnMisiones.Visible = true
			btnAlgoritmo.Visible = true
			btnMapa.Visible = true
			modoActual = "VISUAL"
			print("âœ… MODO VISUAL ACTIVADO")

		elseif modo == "MATRIZ" then
			panelMatrizAdyacencia.Visible = true
			btnAlgoritmo.Visible = true
			modoActual = "MATRIZ"
			print("âœ… MODO MATRIZ ACTIVADO")

		elseif modo == "ANALISIS" then
			overlayAnalisis.Visible = true
			modoActual = "ANALISIS"
			print("âœ… MODO ANÃLISIS ACTIVADO")
		end
	end

	-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
	-- CONECTAR BOTONES DE MODOS
	-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
	if botonesModosRef.Visual then
		botonesModosRef.Visual.MouseButton1Click:Connect(function()
			GestorModos:CambiarModo("VISUAL")
		end)
	end

	if botonesModosRef.Matriz then
		botonesModosRef.Matriz.MouseButton1Click:Connect(function()
			GestorModos:CambiarModo("MATRIZ")
		end)
	end

	if botonesModosRef.Analisis then
		botonesModosRef.Analisis.MouseButton1Click:Connect(function()
			GestorModos:CambiarModo("ANALISIS")
		end)
	end

	-- Toggle Misiones
	btnMisiones.MouseButton1Click:Connect(function()
		misionFrame.Visible = not misionFrame.Visible
	end)

	-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
	-- REFERENCIAS DE LA UI
	-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
	local ReferenciasUI = {
		GUI = gui,
		Marcos = {
			BarraSuperior = barraSuperior,
			SelectorModos = selectorModos,
			ContenedorMiniMapa = contenedorMiniMapa,
			PanelMatrizAdyacencia = panelMatrizAdyacencia,
			OverlayAnalisis = overlayAnalisis,
			PanelPuntuacion = panelPuntuacion,
			MisionFrame = misionFrame,
		},
		Botones = {
			BtnAlgoritmo = btnAlgoritmo,
			BtnMapa = btnMapa,
			BtnMatriz = btnMatriz,
			BtnMisiones = btnMisiones,
			BtnReiniciar = btnReiniciar,
			BtnFinalizar = btnFinalizar,
		},
		Etiquetas = {
			ValorEstrellas = valorEstrellas,
