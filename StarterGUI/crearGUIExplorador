	-- âœ¨ Conectar el botÃ³n de cierre
	botonCerrarAnalisis.MouseButton1Click:Connect(function()
		local animacion = ServicioTween:Create(
			overlayAnalisis,
			TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.In),
			{BackgroundTransparency = Tema.Alfa.Pesado + 0.2}
