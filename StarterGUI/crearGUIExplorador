--[[
	â•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—
	â•‘          EXPLORADOR DE GRAFOS - GUI UNIFICADA                â•‘
	â•‘          Script Refactorizado y Mejorado                      â•‘
	â•‘          Modo Visual | Matriz | AnÃ¡lisis                     â•‘
	â•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
	Este es un script temporal para crear la GUI 
]]

local function crearGUIExplorador()
	-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
	-- SERVICIOS
	-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
	local ServicioTween = game:GetService("TweenService")
	local ServicioGui = game:GetService("StarterGui")
	local ServicioAlmacenamiento = game:GetService("ReplicatedStorage")
	local ServicioJugadores = game:GetService("Players")
	local jugadorLocal = ServicioJugadores.LocalPlayer

	-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
	-- TEMA Y CONFIGURACIÃ“N
	-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
	local Tema = {
		Colores = {
			Fondo = Color3.fromRGB(20, 22, 28),
			Superficie = Color3.fromRGB(30, 33, 42),
			Overlay = Color3.fromRGB(12, 13, 17),
			Visual = Color3.fromRGB(65, 180, 137),
			Matriz = Color3.fromRGB(100, 160, 215),
			Analisis = Color3.fromRGB(215, 140, 60),
			Exito = Color3.fromRGB(76, 175, 80),
			Advertencia = Color3.fromRGB(255, 193, 7),
			Peligro = Color3.fromRGB(229, 57, 53),
			Informacion = Color3.fromRGB(33, 150, 243),
			TextoPrimario = Color3.fromRGB(240, 245, 250),
			TextoSecundario = Color3.fromRGB(176, 190, 197),
			TextoMuteo = Color3.fromRGB(120, 135, 150),
			Cuadricula = Color3.fromRGB(45, 52, 65),
		},
		Fuente = {
			Pantalla = Enum.Font.GothamBold,
			Negrita = Enum.Font.GothamBold,
			Regular = Enum.Font.Gotham,
			Mono = Enum.Font.Code,
		},
		Alfa = {
			Solido = 0,
			Sutil = 0.15,
			Moderado = 0.3,
			Alto = 0.5,
			Pesado = 0.7,
		},
		RadioEsquina = 8,
		VelAnimacion = 0.2,
	}

	-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
	-- FACTORY DE COMPONENTES
	-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
	local FabricaComponentes = {}

	function FabricaComponentes.Marco(nombre, padre, tamanio, posicion, transparente, colorFondo)
		local marco = Instance.new("Frame")
		marco.Name = nombre
		marco.Size = tamanio
		marco.Position = posicion or UDim2.new(0, 0, 0, 0)
		marco.BackgroundColor3 = colorFondo or (transparente and Color3.new(0, 0, 0) or Tema.Colores.Superficie)
		marco.BackgroundTransparency = transparente and Tema.Alfa.Pesado or Tema.Alfa.Sutil
		marco.BorderSizePixel = 0
		marco.Parent = padre

		local esquina = Instance.new("UICorner")
		esquina.CornerRadius = UDim.new(0, Tema.RadioEsquina)
		esquina.Parent = marco

		return marco
	end

	function FabricaComponentes.Etiqueta(nombre, padre, texto, color, tamanioFuente, fuente)
		local etiqueta = Instance.new("TextLabel")
		etiqueta.Name = nombre
		etiqueta.Text = texto
		etiqueta.BackgroundTransparency = 1
		etiqueta.TextColor3 = color or Tema.Colores.TextoPrimario
		etiqueta.Font = fuente or Tema.Fuente.Regular
		etiqueta.TextSize = tamanioFuente or 14
		etiqueta.Parent = padre
		return etiqueta
	end

	function FabricaComponentes.Boton(nombre, padre, texto, tamanio, color)
		local boton = Instance.new("TextButton")
		boton.Name = nombre
		boton.Text = texto
		boton.Size = tamanio or UDim2.new(0, 120, 0, 40)
		boton.BackgroundColor3 = color or Tema.Colores.Matriz
		boton.BackgroundTransparency = Tema.Alfa.Sutil
		boton.TextColor3 = Tema.Colores.TextoPrimario
		boton.Font = Tema.Fuente.Negrita
		boton.TextSize = 12
		boton.AutoButtonColor = false
		boton.Parent = padre

		local esquina = Instance.new("UICorner")
		esquina.CornerRadius = UDim.new(0, 6)
		esquina.Parent = boton

		local colorOriginal = boton.BackgroundColor3
		local transpOriginal = boton.BackgroundTransparency

		-- âœ¨ AnimaciÃ³n al pasar el mouse
		boton.MouseEnter:Connect(function()
			local animacion = ServicioTween:Create(
				boton,
				TweenInfo.new(Tema.VelAnimacion, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
				{BackgroundTransparency = transpOriginal - 0.1}
			)
			animacion:Play()
		end)

		boton.MouseLeave:Connect(function()
			local animacion = ServicioTween:Create(
				boton,
				TweenInfo.new(Tema.VelAnimacion, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
				{BackgroundTransparency = transpOriginal}
			)
			animacion:Play()
		end)

		return boton
	end

	function FabricaComponentes.BotonCerrar(nombre, padre, color)
		local boton = Instance.new("TextButton")
		boton.Name = nombre
		boton.Text = "âœ•"
		boton.Size = UDim2.new(0, 35, 0, 35)
		boton.BackgroundColor3 = color or Tema.Colores.Peligro
		boton.BackgroundTransparency = Tema.Alfa.Sutil
		boton.TextColor3 = Tema.Colores.TextoPrimario
		boton.Font = Tema.Fuente.Negrita
		boton.TextSize = 20
		boton.AutoButtonColor = false
		boton.Parent = padre

		local esquina = Instance.new("UICorner")
		esquina.CornerRadius = UDim.new(0, 6)
		esquina.Parent = boton

		boton.MouseEnter:Connect(function()
			local animacion = ServicioTween:Create(
				boton,
				TweenInfo.new(Tema.VelAnimacion, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
				{BackgroundTransparency = Tema.Alfa.Sutil - 0.15}
			)
			animacion:Play()
		end)

		boton.MouseLeave:Connect(function()
			local animacion = ServicioTween:Create(
				boton,
				TweenInfo.new(Tema.VelAnimacion, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
				{BackgroundTransparency = Tema.Alfa.Sutil}
			)
			animacion:Play()
		end)

		return boton
	end

	-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
	-- INICIALIZACIÃ“N DE GUI PRINCIPAL
	-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
	if ServicioGui:FindFirstChild("GUIExplorador") then
		ServicioGui.GUIExplorador:Destroy()
	end

	local gui = Instance.new("ScreenGui")
	gui.Name = "GUIExplorador"
	gui.ResetOnSpawn = false
	gui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
	gui.Parent = ServicioGui
	gui.Enabled = false

	-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
	-- BARRA SUPERIOR (TOP BAR)
	-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
	local barraSuperior = FabricaComponentes.Marco("BarraSuperior", gui, UDim2.new(1, -40, 0, 60), UDim2.new(0, 20, 0, 15))

	local distribucionBarraSuperior = Instance.new("UIListLayout")
	distribucionBarraSuperior.FillDirection = Enum.FillDirection.Horizontal
	distribucionBarraSuperior.HorizontalAlignment = Enum.HorizontalAlignment.Left
	distribucionBarraSuperior.VerticalAlignment = Enum.VerticalAlignment.Center
	distribucionBarraSuperior.Padding = UDim.new(0, 16)
	distribucionBarraSuperior.Parent = barraSuperior

	local etiquetaTitulo = FabricaComponentes.Etiqueta("Titulo", barraSuperior, "ğŸ“Š Explorador de Grafos", Tema.Colores.TextoPrimario, 20, Tema.Fuente.Pantalla)
	etiquetaTitulo.Size = UDim2.new(0.3, 0, 1, 0)

		-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
	-- PANEL DE PUNTUACIÃ“N (VERSIÃ“N MEJORADA)
	-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
	local panelPuntuacion = FabricaComponentes.Marco("PanelPuntuacion", barraSuperior, UDim2.new(0.4, 0, 1, 0), UDim2.new(0.5, 0, 0, 0))
	
	-- Separador visual
	local separador = Instance.new("Frame")
	separador.Name = "Separador"
	separador.Size = UDim2.new(0, 2, 0.6, 0)
	separador.Position = UDim2.new(0, 10, 0.2, 0)
	separador.BackgroundColor3 = Tema.Colores.Matriz
	separador.BackgroundTransparency = 0.5
	separador.BorderSizePixel = 0
	separador.Parent = panelPuntuacion

	local distribucionPuntuacion = Instance.new("UIListLayout")
	distribucionPuntuacion.FillDirection = Enum.FillDirection.Horizontal
	distribucionPuntuacion.HorizontalAlignment = Enum.HorizontalAlignment.Center
	distribucionPuntuacion.VerticalAlignment = Enum.VerticalAlignment.Center
	distribucionPuntuacion.Padding = UDim.new(0, 30)
	distribucionPuntuacion.Parent = panelPuntuacion

	-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
	-- CONTENEDOR DE ESTRELLAS (MEJORADO)
	-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
	local contenedorEstrellas = Instance.new("Frame")
	contenedorEstrellas.Name = "ContenedorEstrellas"
	contenedorEstrellas.Size = UDim2.new(0, 140, 1, 0)
	contenedorEstrellas.BackgroundTransparency = 1
	contenedorEstrellas.Parent = panelPuntuacion

	-- Marco decorativo para estrellas
	local marcoEstrellas = Instance.new("Frame")
	marcoEstrellas.Name = "MarcoEstrellas"
	marcoEstrellas.Size = UDim2.new(1, -10, 0.85, 0)
	marcoEstrellas.Position = UDim2.new(0, 5, 0.075, 0)
	marcoEstrellas.BackgroundColor3 = Tema.Colores.Overlay
	marcoEstrellas.BackgroundTransparency = 0.5
	marcoEstrellas.BorderSizePixel = 0
	marcoEstrellas.Parent = contenedorEstrellas

	local esquinaMarcoEst = Instance.new("UICorner")
	esquinaMarcoEst.CornerRadius = UDim.new(0, 8)
	esquinaMarcoEst.Parent = marcoEstrellas

	local etiquetaEstrellas = FabricaComponentes.Etiqueta("Etiqueta", marcoEstrellas, "â­ ESTRELLAS", Tema.Colores.Advertencia, 11, Tema.Fuente.Negrita)
	etiquetaEstrellas.Size = UDim2.new(1, 0, 0.35, 0)
	etiquetaEstrellas.TextXAlignment = Enum.TextXAlignment.Center
	etiquetaEstrellas.BackgroundTransparency = 1

	local valorEstrellas = FabricaComponentes.Etiqueta("Valor", marcoEstrellas, "â˜†â˜†â˜†", Tema.Colores.Advertencia, 18, Tema.Fuente.Negrita)
	valorEstrellas.Size = UDim2.new(1, 0, 0.65, 0)
	valorEstrellas.Position = UDim2.new(0, 0, 0.35, 0)
	valorEstrellas.TextXAlignment = Enum.TextXAlignment.Center
	valorEstrellas.BackgroundTransparency = 1

	-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
	-- CONTENEDOR DE PUNTOS (MEJORADO)
	-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
	local contenedorPuntos = Instance.new("Frame")
	contenedorPuntos.Name = "ContenedorPuntos"
	contenedorPuntos.Size = UDim2.new(0, 160, 1, 0)
	contenedorPuntos.BackgroundTransparency = 1
	contenedorPuntos.Parent = panelPuntuacion

	-- Marco decorativo para puntos
	local marcoPuntos = Instance.new("Frame")
	marcoPuntos.Name = "MarcoPuntos"
	marcoPuntos.Size = UDim2.new(1, -10, 0.85, 0)
	marcoPuntos.Position = UDim2.new(0, 5, 0.075, 0)
	marcoPuntos.BackgroundColor3 = Tema.Colores.Overlay
	marcoPuntos.BackgroundTransparency = 0.5
	marcoPuntos.BorderSizePixel = 0
	marcoPuntos.Parent = contenedorPuntos

	local esquinaMarcoP = Instance.new("UICorner")
	esquinaMarcoP.CornerRadius = UDim.new(0, 8)
	esquinaMarcoP.Parent = marcoPuntos

	local etiquetaPuntos = FabricaComponentes.Etiqueta("Etiqueta", marcoPuntos, "ğŸ† PUNTOS", Tema.Colores.Informacion, 11, Tema.Fuente.Negrita)
	etiquetaPuntos.Size = UDim2.new(1, 0, 0.35, 0)
	etiquetaPuntos.TextXAlignment = Enum.TextXAlignment.Center
	etiquetaPuntos.BackgroundTransparency = 1

	local valorPuntos = FabricaComponentes.Etiqueta("Valor", marcoPuntos, "0", Tema.Colores.Informacion, 20, Tema.Fuente.Negrita)
	valorPuntos.Size = UDim2.new(1, 0, 0.65, 0)
	valorPuntos.Position = UDim2.new(0, 0, 0.35, 0)
	valorPuntos.TextXAlignment = Enum.TextXAlignment.Center
	valorPuntos.BackgroundTransparency = 1

	-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
	-- CONTENEDOR DE DINERO (NUEVO)
	-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
	local contenedorDinero = Instance.new("Frame")
	contenedorDinero.Name = "ContenedorDinero"
	contenedorDinero.Size = UDim2.new(0, 140, 1, 0)
	contenedorDinero.BackgroundTransparency = 1
	contenedorDinero.Parent = panelPuntuacion

	-- Marco decorativo para dinero
	local marcoDinero = Instance.new("Frame")
	marcoDinero.Name = "MarcoDinero"
	marcoDinero.Size = UDim2.new(1, -10, 0.85, 0)
	marcoDinero.Position = UDim2.new(0, 5, 0.075, 0)
	marcoDinero.BackgroundColor3 = Tema.Colores.Overlay
	marcoDinero.BackgroundTransparency = 0.5
	marcoDinero.BorderSizePixel = 0
	marcoDinero.Parent = contenedorDinero

	local esquinaMarcoD = Instance.new("UICorner")
	esquinaMarcoD.CornerRadius = UDim.new(0, 8)
	esquinaMarcoD.Parent = marcoDinero

	local etiquetaDinero = FabricaComponentes.Etiqueta("Etiqueta", marcoDinero, "ğŸ’° DINERO", Tema.Colores.Exito, 11, Tema.Fuente.Negrita)
	etiquetaDinero.Size = UDim2.new(1, 0, 0.35, 0)
	etiquetaDinero.TextXAlignment = Enum.TextXAlignment.Center
	etiquetaDinero.BackgroundTransparency = 1

	local valorDinero = FabricaComponentes.Etiqueta("Valor", marcoDinero, "$0", Tema.Colores.Exito, 18, Tema.Fuente.Negrita)
	valorDinero.Size = UDim2.new(1, 0, 0.65, 0)
	valorDinero.Position = UDim2.new(0, 0, 0.35, 0)
	valorDinero.TextXAlignment = Enum.TextXAlignment.Center
	valorDinero.BackgroundTransparency = 1
	
	-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
	-- BARRA DE BOTONES PRINCIPALES
	-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
	-- Se mueve debajo de la barra superior (Y = 85)
	local barraBotonesMain = FabricaComponentes.Marco("BarraBotonesMain", gui, UDim2.new(0, 500, 0, 60), UDim2.new(0.5, -250, 0, 85))

	local distribucionBotonesMain = Instance.new("UIListLayout")
	distribucionBotonesMain.FillDirection = Enum.FillDirection.Horizontal
	distribucionBotonesMain.HorizontalAlignment = Enum.HorizontalAlignment.Center
	distribucionBotonesMain.VerticalAlignment = Enum.VerticalAlignment.Center
	distribucionBotonesMain.Padding = UDim.new(0, 8)
	distribucionBotonesMain.Parent = barraBotonesMain

	local btnAlgoritmo = FabricaComponentes.Boton("BtnAlgoritmo", barraBotonesMain, "ğŸ¯ Algoritmo", UDim2.new(0, 115, 0, 45), Tema.Colores.Analisis)
	local btnMapa = FabricaComponentes.Boton("BtnMapa", barraBotonesMain, "ğŸ—ºï¸ Mapa", UDim2.new(0, 90, 0, 45), Tema.Colores.Visual)
	local btnMatriz = FabricaComponentes.Boton("BtnMatriz", barraBotonesMain, "ğŸ“‹ Matriz", UDim2.new(0, 100, 0, 45), Tema.Colores.Matriz)
